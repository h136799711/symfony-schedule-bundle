{
    "name": "m-adamski/symfony-schedule-bundle",
    "description": "Schedule Bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Adamski",
            "email": "m.adamski@ideacode.pl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "dragonmantank/cron-expression": "^2.0",
        "symfony/console": "^3.4|^4.1",
        "symfony/framework-bundle": "^3.4|^4.1",
        "symfony/lock": "^3.4|^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Adamski\\Symfony\\ScheduleBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
